<!DOCTYPE html>

<html>
<head>
   
    <title>@ViewBag.Title</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <link href="~/Content/css/reset.css" rel="stylesheet" />


    <script src="~/Scripts/jquery-1.11.3.min.js"></script>
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/jquery-ui.css" rel="stylesheet" />
    <link href="~/Content/css/root.css" rel="stylesheet" />
    @*<script src="~/Content/ckeditor/config.js"></script>
    <script src="~/Content/ckeditor/ckeditor.js"></script>
    <script src="~/Content/ckfinder/ckfinder.js"></script>*@
    <script src="~/Content/css/ckeditor/config.js"></script>
    <script src="~/Content/css/ckeditor/ckeditor.js"></script>
    <script src="~/Content/css/ckfinder/ckfinder.js"></script>
    <link href="~/Content/css/paging.css" rel="stylesheet" />
    <link href="~/Content/style.css" rel="stylesheet" />
    <link href="~/Content/css/sb-admin.css" rel="stylesheet" />
    <link href="/Content/css/plugins/dataTables.bootstrap.css" rel="stylesheet">
    <link href="~/Content/Csspublic/css/styles.css" rel="stylesheet" />>
    <script src="~/Scripts/jquery-2.0.0.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/jquery-ui-1.8.20.min.js"></script>
    <script src="~/Scripts/jquery.min.js"></script>
    <script src="~/Scripts/jquery-ui-1.8.11.custom.min.js"></script>
    <link href="~/Content/font-awesome-4.1.0/css/font-awesome.css" rel="stylesheet" />
    <link href="~/Content/font-awesome-4.1.0/css/font-awesome.min.css" rel="stylesheet" />
    <script src="~/Scripts/jquery.ui.slider.js"></script>
    <script src="~/Scripts/jquery.ui.datepicker.js"></script>
    <script src="~/Scripts/toogle.js"></script>
    <script src="~/Scripts/MemberValidte.js"></script>
    <script src="~/Scripts/angular.min.js"></script>
</head>
    <body>
        <div class="overbackgroud" id="overbackgroud" style="display: none;">
            <img src="/img/loading/6.gif" style="position: relative; top: 44%; left: 46%" />
        </div>
        <div id="wrapper">
    
            <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
                @Html.Partial("_PartialMenuTop")
                @Html.Partial("_PartialMenuLeft")
            </nav>
            <div id="page-wrapper" style="margin-top: 45px;">

                @RenderBody()
            </div>
  
        </div>
    
  
        @*<script src="/Scripts/jquery.js"></script>*@
        <!-- Bootstrap Core JavaScript -->
        @*<script src="/Scripts/bootstrap.min.js"></script>*@
        <script src="/Scripts/bootstrap.min.js"></script>
        <script type="text/javascript" src="/Scripts/jquery.ui.datepicker.js"></script>
        <script src="~/Scripts/jquery.dataTables.js"></script>
        <!-- Morris Cha/rts JavaScript -->
        <script src="/Scripts/plugins/morris/raphael.min.js"></script>
        <script src="/Scripts/plugins/morris/morris.min.js"></script>
        <script src="/Scripts/plugins/morris/morris-data.js"></script>
        <style>
            input.ng-invalid {
                border: 1px solid red;
            }

            input.ng-valid {
                border: 1px solid green;
            }
            #page-wrapper {
                min-height: 600px;
            }
            .modal-body {
                margin: auto;
                width: 96%;
            }
            .nav.navbar-nav.side-nav img {
                width: 25px;
            }

        </style>
       
    </body>
</html>
