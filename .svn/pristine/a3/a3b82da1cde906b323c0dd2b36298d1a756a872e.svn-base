.progress {
    box-shadow: none;
    background: #e6e9ee;
    border-radius: 20px;
    overflow: inherit;
}

    .progress .progress-bar {
        box-shadow: none;
        text-transform: uppercase;
        position: relative;
        border-radius: 20px;
    }

    .progress .tooltip {
        z-index: 1000;
        opacity: 1;
        right: -18px;
        width: 45px;
        top: -30px;
        height: 33px;
    }

        .progress .tooltip .tooltip-circle {
            width: 20px;
            height: 20px;
            margin-top: 5px;
        }

    .progress.sm .tooltip .tooltip-circle {
        width: 10px;
        height: 10px;
        margin-top: 1px;
    }

    .progress.xs .tooltip .tooltip-circle {
        width: 7px;
        height: 7px;
        margin-top: -2px;
    }

    .progress.lg .tooltip {
        right: -14px;
    }

        .progress.lg .tooltip .tooltip-circle {
            width: 28px;
            height: 28px;
            margin-top: 5px;
        }

