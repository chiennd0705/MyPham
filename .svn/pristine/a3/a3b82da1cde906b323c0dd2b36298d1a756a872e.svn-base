/* Breadcrumb */

.breadcrumb-custom {
    background: none;
    padding-left: 0;
    padding-right: 0px;
}

    .breadcrumb-custom li {
        position: relative;
    }

        .breadcrumb-custom li a {
            color: #aab2bd;
            padding-right: 18px;
            font-family: "Roboto";
        }

    .breadcrumb-custom > .active {
        color: #e51c23 !important;
    }

.breadcrumb > li + li:before {
    content: inherit;
}

.icon-bread-arrow {
    position: absolute;
    right: 5px;
    top: 4px;
}
/* Progress step 1 */

.progress-step {
    display: block;
    height: 20px;
    background: #e6e9ee;
    border-radius: 20px;
    overflow: hidden;
    margin-bottom: 15px;
}

    .progress-step ul {
        margin: 0px;
        padding-left: 0px;
    }

        .progress-step ul li {
            display: inline;
            position: relative;
        }

            .progress-step ul li a {
                color: #656d78;
                text-transform: uppercase;
                font-size: 12px;
                display: block;
                text-align: center;
                line-height: 20px;
                font-family: "Roboto";
            }

            .progress-step ul li.active {
                background: #a0d569;
            }

                .progress-step ul li.active a {
                    color: #fff;
                }

            .progress-step ul li.focus {
                border-top-right-radius: 20px;
                -moz-border-top-right-radius: 20px;
                -webkit-border-top-right-radius: 20px;
                border-bottom-right-radius: 20px;
                -moz-border-bottom-right-radius: 20px;
                -webkit-border-bottom-right-radius: 20px;
            }
/*  Progress step 2 */

.progress-style2 {
    width: 100%;
    margin: 20px auto;
    text-align: center;
}

    .progress-style2 .circle, .progress-style2 .bar {
        display: inline-block;
        width: 40px;
        height: 40px;
        border-radius: 40px;
    }

    .progress-style2 .bar {
        position: relative;
        width: 18%;
        height: 10px;
        margin: 0 -12px 27px;
        border-left: none;
        border-right: none;
        border-radius: 0;
        background: #e6e9ee;
        color: #666867;
    }

    .progress-style2 .circle .label {
        display: inline-block;
        width: 25px;
        height: 25px;
        line-height: 25px;
        border-radius: 28px;
        color: #666867;
        font-size: 12px;
        position: relative;
        z-index: 100;
        padding: 0px;
        text-align: center;
        background: #e6e9ee;
    }

    .progress-style2 .circle .title {
        color: #656d78;
        font-size: 12px;
        line-height: 30px;
        margin-left: -5px;
        text-transform: uppercase;
    }
    /* Done / Active */

    .progress-style2 .bar.done {
        background: #a0d569;
    }

    .progress-style2 .bar.active {
        background: linear-gradient(to right, #EEE 40%, #FFF 60%);
    }

    .progress-style2 .circle.done .label {
        color: #FFF;
        background: #a0d569;
    }

    .progress-style2 .circle.done .title {
        color: #444;
    }

    .progress-style2 .circle.active .label {
        color: #FFF;
        background: #0c95be;
    }

    .progress-style2 .circle.active .title {
        color: #0c95be;
    }
/* Blockquote */

blockquote {
    border-left: 5px solid #9fd566;
    padding: 0px 10px;
    margin-left: 35px;
}

    blockquote p {
        font-size: 13px;
        line-height: 1.6;
    }

    blockquote small, blockquote .small {
        color: #616d79;
        font-style: italic;
        font-size: 13px;
    }
