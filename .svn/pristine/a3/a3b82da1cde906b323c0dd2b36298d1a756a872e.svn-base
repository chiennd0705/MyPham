.adr-oms.radio {
    width: auto;
    height: auto;
    margin: 0;
    display: block;
}
/*
 * Radio
 */
.adr-oms.radio {
    margin-top: 8px;
    margin-bottom: 8px;
}

    .adr-oms.radio:first-child {
        margin-top: 0;
    }

    .adr-oms.radio:last-child {
        margin-bottom: 0;
    }

    .adr-oms.radio input {
        display: none;
        *display: block;
        *margin: 0;
    }

@media \0screen {
    .adr-oms.radio input {
        display: block;
        margin-left: 0;
    }
}

.adr-oms.radio > label {
    display: inline-block;
    position: relative;
    margin-right: 14px;
    padding-left: 21px !important;
    cursor: pointer;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
}

@media \0screen {
    .adr-oms.radio > label {
        padding-left: 0;
    }
}

.adr-oms.radio > label:before {
    content: "";
    display: block;
    position: absolute;
    width: 14px;
    height: 14px;
    left: 0;
    top: 50%;
    margin-top: -7px;
    border: 1px solid #666666;
    border-radius: 7px;
}

@media \0screen {
    .adr-oms.radio > label:before {
        display: none;
    }
}

.adr-oms.radio > label:after {
    content: "";
    display: block;
    position: absolute;
    width: 0;
    height: 0;
    top: 50%;
    left: 7px;
    margin-top: 0;
    background: #ffffff;
    border-radius: 3px;
    transition: 0.2s ease-in-out;
}

@media \0screen {
    .adr-oms.radio > label:after {
        display: none;
    }
}

.adr-oms.radio > label:hover:before {
    border: 1px solid #0388cd;
}

.adr-oms.radio > label:hover:after {
    background: #ffffff;
}

.adr-oms.radio :checked + label:before {
    border: 1px solid #0388cd;
    background: #0388cd;
}

.adr-oms.radio :checked + label:after {
    color: #ffffff;
    height: 6px;
    width: 6px;
    margin-top: -3px;
    left: 4px;
}

.adr-oms.radio [disabled] + label {
    cursor: not-allowed;
}

    .adr-oms.radio [disabled] + label:before {
        border: 1px solid #cccccc;
        background: #f0f0f0;
    }

    .adr-oms.radio [disabled] + label:after {
        color: #ffffff;
        height: 6px;
        width: 6px;
        margin-top: -3px;
        left: 4px;
    }

