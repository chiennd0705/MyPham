/*Order*/
.text-center {
    text-align: center;
}

.checkout-process .steps {
    background: #eef2f5;
    overflow: hidden;
    width: 102%;
    list-style: none;
    padding-left: 0;
    border-radius: 4px;
    overflow: hidden;
}

    .checkout-process .steps a {
        padding: .7em 1em .7em 1.9em;
        float: left;
        text-decoration: none;
        color: #2c508a;
        position: relative;
        background-color: #eef2f5;
        font-size: 15px;
        height: 40px;
        width: 327px;
    }

.bullet-checkout {
    width: 24px;
    height: 24px;
    display: inline-block;
    text-align: center;
    line-height: 24px;
    position: relative;
}

#top_navigation .steps {
    height: 40px;
    overflow: hidden;
    margin-bottom: 0;
    
}

.checkout-process .steps li {
    float: left;
    width: 33%;
}

.checkout-4-step .checkout-process .steps a {
    padding: .5em 2.1em .5em 3em;
}

.checkout-process .steps li:first-child a {
    padding-left: 1em;
}

.checkout-process .steps a::after {
    z-index: 2;
    border-left-color: #f1f1f1;
}

#top_navigation {
    overflow: hidden;
    border-radius: 4px;
    margin:20px 0;
}

    #top_navigation .steps > li.active > a:after {
        background-color: #f0f0f0!important;
        border-left-color: #f4f9fd;
    }

    #top_navigation .steps > li > a {
        width: 100%;
        padding-top: 0;
        padding-bottom: 0;
        line-height: 40px;
        background-color: #f0f0f0!important;
        color: #333!important;
        font-size: 13px!important;
        text-align: left;
        font-weight: 400;
    }

.complete .number {
    display: none;
}

.checkout-process .steps a.disabled {
    color: #2c508a;
}

#top_navigation .steps > li > a > .bullet-checkout {
    color: #fff!important;
    vertical-align: top;
    width: 24px;
    height: 40px;
    line-height: 40px;
    background-repeat: no-repeat;
    background-position: center center;
    background-image: url(https://order.adayroi.com/Content/default/img/adr-oms-bullet-checkout-bg-gray.png);
}

#top_navigation .steps > li.complete > a > .bullet-checkout {
    background-image: url(https://order.adayroi.com/Content/default/img/adr-oms-bullet-checkout-bg-green-checked.png);
}

.checkout-process .steps a::before {
    border-left-color: #fff;
    right: -1.1em;
    z-index: 1;
}

.checkout-process .steps a::after {
    z-index: 2;
    border-left-color: #f1f1f1;
}

.checkout-process .steps a::after, .checkout-process .steps a::before {
    content: "";
    position: absolute;
    top: 50%;
    margin-top: -1.8em;
    border-top: 1.9em solid transparent;
    border-bottom: 1.8em solid transparent;
    border-left: 1em solid;
    right: -1em;
    color: #f1f1f1;
}

#top_navigation .steps > li.active > a {
    background-color: #f4f9fd!important;
}

    #top_navigation .steps > li.active > a > .bullet-checkout {
        background-image: url(https://order.adayroi.com/Content/default/img/adr-oms-bullet-checkout-bg-blue.png);
    }

.checkout-process .steps li {
    float: left;
}
